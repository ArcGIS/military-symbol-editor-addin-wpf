<UserControl
    x:Class="ProSymbolEditor.MilitarySymbolDockpaneView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Controls="clr-namespace:ArcGIS.Desktop.Extensions.Controls;assembly=ArcGIS.Desktop.Extensions"
    xmlns:extensions="clr-namespace:ArcGIS.Desktop.Extensions;assembly=ArcGIS.Desktop.Extensions"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ProSymbolEditor"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="430"
    d:DesignWidth="550"
    mc:Ignorable="d">
    <UserControl.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <extensions:DesignOnlyResourceDictionary Source="pack://application:,,,/ArcGIS.Desktop.Framework;component\Themes\Default.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <local:StringCharacterValueConverter x:Key="StringCharacterValueConverter" />
            <BooleanToVisibilityConverter x:Key="BoolToVis" />

            <DataTemplate x:Key="NameTokenTemplate">
                <DataTemplate.Resources>
                    <Storyboard x:Key="OnLoaded1">
                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="(UIElement.Opacity)">
                            <SplineDoubleKeyFrame KeyTime="0" Value="0" />
                            <SplineDoubleKeyFrame KeyTime="0:0:0.5" Value="1" />
                        </DoubleAnimationUsingKeyFrames>
                    </Storyboard>
                </DataTemplate.Resources>
                <Border
                    x:Name="border"
                    Height="Auto"
                    Margin="3,0,3,3"
                    Padding="5,3"
                    d:DesignHeight="40"
                    d:DesignWidth="139"
                    BorderBrush="#FF7E7E7E"
                    BorderThickness="2"
                    CornerRadius="5">
                    <Border.Background>
                        <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                            <GradientStop Offset="0" Color="#FFFFD0A0" />
                            <GradientStop Offset="1" Color="#FFAB5600" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Grid Width="Auto" HorizontalAlignment="Left">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.21*" />
                            <ColumnDefinition Width="0.79*" />
                        </Grid.ColumnDefinitions>
                        <Image
                            Grid.Column="0"
                            Width="Auto"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Center"
                            Source="14-tag.png"
                            Stretch="None" />
                        <TextBlock
                            Grid.Column="1"
                            Margin="10,0,0,0"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontWeight="Bold"
                            Text="{Binding}"
                            TextWrapping="NoWrap" />
                    </Grid>
                </Border>
                <DataTemplate.Triggers>
                    <EventTrigger RoutedEvent="FrameworkElement.Loaded">
                        <BeginStoryboard Storyboard="{StaticResource OnLoaded1}" />
                    </EventTrigger>
                </DataTemplate.Triggers>
            </DataTemplate>
        </ResourceDictionary>


    </UserControl.Resources>

    <Grid Margin="3,3,3,3">

        <DockPanel>

            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <TabControl
                    x:Name="tabControl"
                    Grid.Row="0"
                    Margin="3,3,3,3"
                    SelectedIndex="{Binding SelectedTabIndex, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                    <TabItem Header="Search">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <ScrollViewer Grid.Row="0">
                                <StackPanel>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition MinHeight="120" MaxHeight="240" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>

                                        <TextBox
                                            x:Name="searchSymbolsTextBox"
                                            Grid.Row="0"
                                            Height="23"
                                            Margin="5,5,5,5"
                                            HorizontalAlignment="Stretch"
                                            Text="{Binding SearchString, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Delay=2000}">
                                            <TextBox.Style>
                                                <Style xmlns:sys="clr-namespace:System;assembly=mscorlib" TargetType="TextBox">
                                                    <Style.Resources>
                                                        <VisualBrush
                                                            x:Key="CueBannerBrush"
                                                            AlignmentX="Left"
                                                            AlignmentY="Center"
                                                            Stretch="None">
                                                            <VisualBrush.Visual>
                                                                <Label
                                                                    Width="10000"
                                                                    Background="White"
                                                                    Content="Search..."
                                                                    Foreground="Black" />
                                                            </VisualBrush.Visual>
                                                        </VisualBrush>
                                                    </Style.Resources>
                                                    <Style.Triggers>
                                                        <Trigger Property="Text" Value="{x:Static sys:String.Empty}">
                                                            <Setter Property="Background" Value="{StaticResource CueBannerBrush}" />
                                                        </Trigger>
                                                        <Trigger Property="Text" Value="{x:Null}">
                                                            <Setter Property="Background" Value="{StaticResource CueBannerBrush}" />
                                                        </Trigger>
                                                        <Trigger Property="IsKeyboardFocused" Value="True">
                                                            <Setter Property="Background" Value="White" />
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </TextBox.Style>
                                        </TextBox>
                                        <Button
                                            Grid.Row="0"
                                            Height="{Binding Height, ElementName=searchSymbolsTextBox}"
                                            Margin="{Binding Margin, ElementName=searchSymbolsTextBox}"
                                            HorizontalAlignment="Right"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            Background="{Binding Backgound, ElementName=searchSymbolsTextBox}"
                                            Command="{Binding ClearSearchTextCommand}"
                                            Content="X" />

                                        <ListBox
                                            x:Name="symbolListBox"
                                            Grid.Row="1"
                                            MaxHeight="240"
                                            Margin="5,5,5,5"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            ItemContainerStyle="{DynamicResource Esri_HighlightListBoxItem}"
                                            ItemsSource="{Binding StyleItems, UpdateSourceTrigger=PropertyChanged}"
                                            ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                            SelectedItem="{Binding SelectedStyleItem, Mode=TwoWay}">

                                            <ListBox.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <VirtualizingStackPanel>
                                                        <!--  Memory footprint is only 200 mb  -->
                                                    </VirtualizingStackPanel>
                                                </ItemsPanelTemplate>
                                            </ListBox.ItemsPanel>

                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid Margin="2,0" ToolTip="{Binding Name}">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="16" />
                                                            <ColumnDefinition Width="*" />
                                                        </Grid.ColumnDefinitions>
                                                        <Image
                                                            Grid.Column="0"
                                                            Source="{Binding PreviewImage}"
                                                            Stretch="Fill" />
                                                        <TextBlock
                                                            Grid.Column="1"
                                                            Padding="5,1,5,1"
                                                            Text="{Binding Name}"
                                                            TextAlignment="Left"
                                                            TextTrimming="CharacterEllipsis" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>

                                        <Grid Grid.Row="2" Margin="5,0,0,0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>

                                            <Label Grid.Column="0" Content="Result Count:" />
                                            <Label Grid.Column="1" Content="{Binding ResultCount, UpdateSourceTrigger=PropertyChanged}" />
                                        </Grid>

                                    </Grid>

                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>

                                        <Image
                                            x:Name="searchTabImage"
                                            Grid.Row="0"
                                            Width="180"
                                            Height="180"
                                            Margin="20,1,20,1"
                                            VerticalAlignment="Top"
                                            RenderOptions.BitmapScalingMode="HighQuality"
                                            Source="{Binding SymbolAttributeSet.SymbolImage}">
                                            <Image.ContextMenu>
                                                <ContextMenu>
                                                    <MenuItem Command="{Binding SaveImageToCommand}" Header="Save Image As..." />
                                                    <MenuItem Command="{Binding CopyImageToClipboardCommand}" Header="Copy Image to Clipboard" />
                                                </ContextMenu>
                                            </Image.ContextMenu>
                                        </Image>

                                        <ItemsControl
                                            Name="tStack"
                                            Grid.Row="1"
                                            Margin="0,0,0,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            ItemsSource="{Binding SelectedStyleTags, UpdateSourceTrigger=PropertyChanged}">
                                            <ItemsControl.ItemTemplate>
                                                <DataTemplate>
                                                    <Border
                                                        x:Name="border"
                                                        Height="Auto"
                                                        Margin="3,0,3,3"
                                                        Padding="5,3"
                                                        d:DesignHeight="40"
                                                        d:DesignWidth="139"
                                                        Background="{DynamicResource Esri_Blue1}"
                                                        BorderBrush="{DynamicResource Esri_Blue8}"
                                                        BorderThickness="2"
                                                        CornerRadius="5">
                                                        <Grid Width="Auto" HorizontalAlignment="Left">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="*" />
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock
                                                                Grid.Column="0"
                                                                Margin="5,0,5,0"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                FontSize="10"
                                                                Text="{Binding}"
                                                                TextWrapping="Wrap"
                                                                Style="{DynamicResource RegularText}" />
                                                        </Grid>
                                                    </Border>
                                                </DataTemplate>
                                            </ItemsControl.ItemTemplate>

                                            <ItemsControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <WrapPanel />
                                                </ItemsPanelTemplate>
                                            </ItemsControl.ItemsPanel>
                                        </ItemsControl>
                                    </Grid>

                                </StackPanel>
                            </ScrollViewer>

                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                <Button
                                    Grid.Column="0"
                                    Width="90"
                                    HorizontalAlignment="Center"
                                    Command="{Binding SaveSymbolFileCommand}"
                                    Content="Add Favorite"
                                    Style="{DynamicResource Esri_SimpleButton}" />
                                <ToggleButton
                                    Name="searchAddToMapButton"
                                    Grid.Column="1"
                                    Width="90"
                                    Height="24"
                                    HorizontalAlignment="Center"
                                    Background="White"
                                    BorderBrush="Black"
                                    BorderThickness="0.5"
                                    Command="{Binding ActivateAddToMapToolCommand}"
                                    Content="Add to Map"
                                    Foreground="Black"
                                    IsChecked="{Binding AddToMapToolEnabled, UpdateSourceTrigger=PropertyChanged}"
                                    IsEnabled="{Binding IsStyleItemSelected}"
                                    ToolTip="Add to Map"
                                    Visibility="{Binding Path=IsAddingNew, Converter={StaticResource BoolToVis}}" />
                                <Button
                                    x:Name="modifyButton"
                                    Grid.Column="2"
                                    Margin="0,0,-26,0"
                                    HorizontalAlignment="Right"
                                    Command="{Binding GoToTabCommand}"
                                    CommandParameter="3"
                                    IsEnabled="{Binding IsStyleItemSelected}"
                                    Style="{DynamicResource Esri_BackButton}"
                                    ToolTip="Go to Symbol Tab">
                                    <Button.RenderTransform>
                                        <ScaleTransform ScaleX="-1" />
                                    </Button.RenderTransform>
                                </Button>
                            </Grid>
                        </Grid>
                    </TabItem>
                    <TabItem Header="Modify">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <ScrollViewer Grid.Row="0">
                                <StackPanel>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition MaxHeight="240" />
                                        </Grid.RowDefinitions>

                                        <Grid Grid.Row="0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>

                                            <Label
                                                Grid.Column="0"
                                                Margin="5,0,0,0"
                                                Content="Use the select tool to select a feature to modify" />

                                            <ToggleButton
                                                Grid.Column="1"
                                                Margin="2,0,0,0"
                                                Command="{Binding SelectToolCommand}"
                                                IsChecked="{Binding SelectToolEnabled}">
                                                <Image
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    Source="pack://application:,,,/MilitarySymbolEditor;component/Images/SelectionSelectTool16.png" />
                                            </ToggleButton>
                                        </Grid>

                                        <ListBox
                                            x:Name="editSelectionList"
                                            Grid.Row="1"
                                            MinHeight="120"
                                            MaxHeight="240"
                                            Margin="5,5,5,5"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            ItemsSource="{Binding SelectedFeaturesCollection, UpdateSourceTrigger=PropertyChanged}"
                                            SelectedItem="{Binding SelectedSelectedFeature, Mode=TwoWay}">

                                            <ListBox.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <VirtualizingStackPanel>
                                                        <!--  Memory footprint is only 200 mb  -->
                                                    </VirtualizingStackPanel>
                                                </ItemsPanelTemplate>
                                            </ListBox.ItemsPanel>

                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid Margin="2,0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition Width="Auto" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock
                                                            Grid.Column="0"
                                                            Text="{Binding FeatureLayerName}"
                                                            TextAlignment="Left" />
                                                        <TextBlock
                                                            Grid.Column="1"
                                                            Text=" : "
                                                            TextAlignment="Left" />
                                                        <TextBlock
                                                            Grid.Column="2"
                                                            Text="{Binding SymbolSetName}"
                                                            TextAlignment="Left" />
                                                        <TextBlock
                                                            Grid.Column="3"
                                                            Text=" : "
                                                            TextAlignment="Left" />
                                                        <TextBlock
                                                            Grid.Column="4"
                                                            Text="{Binding EntityName}"
                                                            TextAlignment="Left" />
                                                        <TextBlock
                                                            Grid.Column="5"
                                                            Text=" : Object ID "
                                                            TextAlignment="Left" />
                                                        <TextBlock
                                                            Grid.Column="6"
                                                            Text="{Binding ObjectId}"
                                                            TextAlignment="Left" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </Grid>

                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>

                                        <Image
                                            x:Name="modifyTabImage"
                                            Grid.Row="0"
                                            Width="180"
                                            Height="180"
                                            Margin="20,1,20,1"
                                            VerticalAlignment="Top"
                                            RenderOptions.BitmapScalingMode="HighQuality"
                                            Source="{Binding EditSelectedFeatureSymbol.SymbolImage}">
                                            <Image.ContextMenu>
                                                <ContextMenu>
                                                    <MenuItem Command="{Binding SaveImageToCommand}" Header="Save Image As..." />
                                                    <MenuItem Command="{Binding CopyImageToClipboardCommand}" Header="Copy Image to Clipboard" />
                                                </ContextMenu>
                                            </Image.ContextMenu>
                                        </Image>

                                        <xctk:PropertyGrid
                                            Grid.Row="1"
                                            Height="150"
                                            Margin="5,5,5,5"
                                            AutoGenerateProperties="False"
                                            Foreground="Black"
                                            IsCategorized="False"
                                            IsReadOnly="True"
                                            SelectedObject="{Binding EditSelectedFeatureSymbol}"
                                            ShowSearchBox="False"
                                            ShowSortOptions="False"
                                            ShowSummary="False">
                                            <xctk:PropertyGrid.PropertyDefinitions>
                                                <xctk:PropertyDefinition
                                                    DisplayName="Name"
                                                    DisplayOrder="0"
                                                    TargetProperties="Name" />
                                                <xctk:PropertyDefinition
                                                    DisplayName="Display Attributes"
                                                    DisplayOrder="1"
                                                    TargetProperties="DisplayAttributes" />
                                                <xctk:PropertyDefinition
                                                    DisplayName="Label Attributes"
                                                    DisplayOrder="2"
                                                    TargetProperties="LabelAttributes" />
                                                <xctk:PropertyDefinition
                                                    DisplayName="Standard"
                                                    DisplayOrder="3"
                                                    TargetProperties="StandardVersion" />
                                            </xctk:PropertyGrid.PropertyDefinitions>
                                        </xctk:PropertyGrid>

                                    </Grid>
                                </StackPanel>
                            </ScrollViewer>

                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                <Button
                                    Grid.Column="2"
                                    Margin="0,0,-26,0"
                                    HorizontalAlignment="Right"
                                    Command="{Binding GoToTabCommand}"
                                    CommandParameter="3"
                                    IsEnabled="{Binding IsStyleItemSelected}"
                                    Style="{DynamicResource Esri_BackButton}"
                                    ToolTip="Go to Symbol Tab">
                                    <Button.RenderTransform>
                                        <ScaleTransform ScaleX="-1" />
                                    </Button.RenderTransform>
                                </Button>
                            </Grid>
                        </Grid>
                    </TabItem>
                    <TabItem HorizontalAlignment="Stretch" Header="Favorites">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <ScrollViewer Grid.Row="0">
                                <StackPanel>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>


                                        <TextBox
                                            Grid.Row="0"
                                            Height="23"
                                            Margin="5,5,5,5"
                                            HorizontalAlignment="Stretch"
                                            Text="{Binding FavoritesSearchFilter, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                            <TextBox.Style>
                                                <Style xmlns:sys="clr-namespace:System;assembly=mscorlib" TargetType="TextBox">
                                                    <Style.Resources>
                                                        <VisualBrush
                                                            x:Key="CueBannerBrush"
                                                            AlignmentX="Left"
                                                            AlignmentY="Center"
                                                            Stretch="None">
                                                            <VisualBrush.Visual>
                                                                <Label Content="Filter By..." Foreground="DarkGray" />
                                                            </VisualBrush.Visual>
                                                        </VisualBrush>
                                                    </Style.Resources>
                                                    <Style.Triggers>
                                                        <Trigger Property="Text" Value="{x:Static sys:String.Empty}">
                                                            <Setter Property="Background" Value="{StaticResource CueBannerBrush}" />
                                                        </Trigger>
                                                        <Trigger Property="Text" Value="{x:Null}">
                                                            <Setter Property="Background" Value="{StaticResource CueBannerBrush}" />
                                                        </Trigger>
                                                        <Trigger Property="IsKeyboardFocused" Value="True">
                                                            <Setter Property="Background" Value="White" />
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </TextBox.Style>
                                        </TextBox>

                                        <Grid Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>

                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>

                                            <ListBox
                                                Grid.Row="0"
                                                Grid.RowSpan="4"
                                                Grid.Column="0"
                                                Height="175"
                                                Margin="5,5,5,5"
                                                HorizontalAlignment="Stretch"
                                                VerticalAlignment="Stretch"
                                                ItemsSource="{Binding FavoritesView, UpdateSourceTrigger=PropertyChanged}"
                                                ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                                SelectedItem="{Binding SelectedFavoriteSymbol, Mode=TwoWay}">

                                                <ListBox.ItemsPanel>
                                                    <ItemsPanelTemplate>
                                                        <WrapPanel />
                                                        <!--<VirtualizingStackPanel>
                                                        -->
                                                        <!--  Memory footprint is only 200 mb  -->
                                                        <!--
                                                    </VirtualizingStackPanel>-->
                                                    </ItemsPanelTemplate>
                                                </ListBox.ItemsPanel>

                                                <ListBox.ItemTemplate>
                                                    <DataTemplate>
                                                        <Image
                                                            Width="100"
                                                            Height="100"
                                                            RenderTransformOrigin="0.5,0.5"
                                                            Source="{Binding SymbolImage}"
                                                            Stretch="UniformToFill">
                                                            <Image.RenderTransform>
                                                                <ScaleTransform ScaleX="-1" ScaleY="-1" />
                                                            </Image.RenderTransform>
                                                        </Image>
                                                    </DataTemplate>
                                                </ListBox.ItemTemplate>
                                            </ListBox>

                                            <Button
                                                Grid.Row="0"
                                                Grid.Column="1"
                                                Margin="5,5,5,5"
                                                Command="{Binding DeleteFavoriteSymbolCommand}"
                                                Content="Delete"
                                                IsEnabled="{Binding IsFavoriteItemSelected}"
                                                Style="{DynamicResource Esri_SimpleButton}" />
                                            <Button
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                Margin="5,5,5,5"
                                                Command="{Binding CreateTemplateFromFavoriteCommand}"
                                                IsEnabled="{Binding IsFavoriteItemSelected}"
                                                Style="{DynamicResource Esri_SimpleButton}">
                                                <!-- Content= (Multiline Content Below)-->
                                                <StackPanel> 
                                                    <TextBlock HorizontalAlignment="Center" Text="Create" />
                                                    <TextBlock HorizontalAlignment="Center" Text="Template" />
                                                </StackPanel>
                                            </Button>
                                            <Button
                                                Grid.Row="2"
                                                Grid.Column="1"
                                                Margin="5,5,5,5"
                                                Command="{Binding ImportFavoritesFileCommand}"
                                                Content="Import..."
                                                Style="{DynamicResource Esri_SimpleButton}" />
                                            <Button
                                                Grid.Row="3"
                                                Grid.Column="1"
                                                Margin="5,5,5,5"
                                                Command="{Binding SaveFavoritesFileAsCommand}"
                                                Content="Save As..."
                                                Style="{DynamicResource Esri_SimpleButton}" />

                                        </Grid>

                                        <ItemsControl
                                            Grid.Row="2"
                                            Margin="0,0,0,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            ItemsSource="{Binding SelectedFavoriteStyleTags, UpdateSourceTrigger=PropertyChanged}">
                                            <ItemsControl.ItemTemplate>
                                                <DataTemplate>
                                                    <Border
                                                        x:Name="border"
                                                        Height="Auto"
                                                        Margin="3,0,3,3"
                                                        Padding="5,3"
                                                        d:DesignHeight="40"
                                                        d:DesignWidth="139"
                                                        Background="{DynamicResource Esri_Blue1}"
                                                        BorderBrush="{DynamicResource Esri_Blue8}"
                                                        BorderThickness="2"
                                                        CornerRadius="5">
                                                        <Grid Width="Auto" HorizontalAlignment="Left">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="*" />
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock
                                                                Grid.Column="0"
                                                                Margin="5,0,5,0"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                FontSize="10"
                                                                Text="{Binding}"
                                                                Style="{DynamicResource RegularText}"
                                                                TextWrapping="Wrap" />
                                                        </Grid>
                                                    </Border>
                                                </DataTemplate>
                                            </ItemsControl.ItemTemplate>

                                            <ItemsControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <WrapPanel />
                                                </ItemsPanelTemplate>
                                            </ItemsControl.ItemsPanel>
                                        </ItemsControl>
                                    </Grid>

                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>

                                        <Image
                                            x:Name="favoritesTabImage"
                                            Grid.Row="0"
                                            Width="180"
                                            Height="180"
                                            Margin="20,1,20,1"
                                            VerticalAlignment="Bottom"
                                            RenderOptions.BitmapScalingMode="HighQuality"
                                            Source="{Binding SelectedFavoriteSymbol.SymbolImage}">
                                            <Image.ContextMenu>
                                                <ContextMenu>
                                                    <MenuItem Command="{Binding SaveImageToCommand}" Header="Save Image As..." />
                                                    <MenuItem Command="{Binding CopyImageToClipboardCommand}" Header="Copy Image to Clipboard" />
                                                </ContextMenu>
                                            </Image.ContextMenu>
                                        </Image>

                                        <xctk:PropertyGrid
                                            Grid.Row="1"
                                            Height="150"
                                            Margin="5,5,5,5"
                                            AutoGenerateProperties="False"
                                            Foreground="Black"
                                            IsCategorized="False"
                                            IsReadOnly="True"
                                            SelectedObject="{Binding SelectedFavoriteSymbol}"
                                            ShowSearchBox="False"
                                            ShowSortOptions="False"
                                            ShowSummary="False">
                                            <xctk:PropertyGrid.PropertyDefinitions>
                                                <xctk:PropertyDefinition
                                                    DisplayName="Name"
                                                    DisplayOrder="0"
                                                    TargetProperties="Name" />
                                                <xctk:PropertyDefinition
                                                    DisplayName="Display Attributes"
                                                    DisplayOrder="1"
                                                    TargetProperties="DisplayAttributes" />
                                                <xctk:PropertyDefinition
                                                    DisplayName="Label Attributes"
                                                    DisplayOrder="2"
                                                    TargetProperties="LabelAttributes" />
                                                <xctk:PropertyDefinition
                                                    DisplayName="Standard"
                                                    DisplayOrder="3"
                                                    TargetProperties="StandardVersion" />
                                            </xctk:PropertyGrid.PropertyDefinitions>
                                        </xctk:PropertyGrid>

                                    </Grid>
                                </StackPanel>
                            </ScrollViewer>

                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
